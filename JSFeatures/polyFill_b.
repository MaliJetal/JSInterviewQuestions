let name = {
  firstName: "Jetal",
  lastName: "Mali"
}

let printName = function(){
  console.log(this.firstName + "" + this.lastName);
}

let printMyName = printName.bind(name);
printMyName();

function.protptype.mybind = function(){
  let obj = this; //this => printName
  return function(){
    obj.call(name);
  }
}

let printMyName2 = printName.mybind(name);
